	"tokenColors": [
		{
			"name": "Base Source",
			"scope": [
				"source",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#bfbfe2"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#fff"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"punctuation.definition.directive",
				"keyword.control.directive",
				"meta.preprocessor",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e1e1ff"
			}
		},
		{
			"name": "Preprocessor content",
			"scope": [
				"entity.name.function.preprocessor",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#e1e1ff"
			}
		},
		{
			"name": "Storage Modifier",
			"scope": [
				"storage.modifier",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e1e1ff"
			}
		},
		{
			"name": "Function Name",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#e1e1ff"
			}
		},
		{
			"name": "Function Parameter",
			"scope": [
				"variable.parameter.probably",
				"meta.function.definition.parameters",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#bfbfe2"
			}
		},
		{
			"name": "Built-In Type",
			"scope": [
				"storage.type.built-in.primitive",
				"storage.type.built-in.c",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#e1e1ff"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#e1e1ff"
			}
		},
		{
			"name": "Number Literal",
			"scope": [
				"constant.numeric",
				"keyword.other.unit.hexadecimal",
				"keyword.other.unit.binary"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#e1e1ff"
			}
		},
		{
			"name": "Logical Operator",
			"scope": [
				"keyword.operator.logical",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e1e1ff"
			}
		},
	],
}
